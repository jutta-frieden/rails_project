
<div class="page-header">
  <h1>Users</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>User email</th>
      <th>Last login</th>
    </tr>
  </thead>

  <tbody>
    <% if @users %>
      <% @users.each do |user| %>
        <tr> 
          <td>
            <% link_to user.email, user %>
          </td>
          <td>
            <% %>
          </td>
        </tr>
      <% end %>
    <% end %>
  </tbody>

</table>